{
  "name": "influx",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./influxClient": "./dist/influxClient/index.js",
    "./sutechEquipment": "./dist/sutechEquipment/index.js",
    "./xgtClient": "./dist/xgtClient/index.json"
  },
  "typesVersions": {
    "*": {
      "influxClient": [
        "dist/influxClient/index.d.ts"
      ],
      "sutechEquipment": [
        "dist/sutechEquipment/index.d.ts"
      ],
      "xgtClient": [
        "dist/xgtClient/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "npm run clean && ./node_modules/.bin/tsc --build tsconfig.build.json",
    "postbuild": "cp package*.json dist && cd dist && npm ci --production",
    "dev": "DEBUG=su-agent ts-node -r dotenv/config src/index.ts",
    "read": "DEBUG=* ts-node -r dotenv/config src/xgtClient/snippets/readSample.ts",
    "write": "DEBUG=* ts-node -r dotenv/config src/xgtClient/snippets/writeSample.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}